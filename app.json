{
  "expo": {
    "name": "BETA",
    "description": "A very interesting bus project.",
    "slug": "busApp",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["android"],
    "version": "0.6.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
